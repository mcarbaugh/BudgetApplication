<div class="loginFormContainer">
    <form method="post" action="loginForm" class="loginForm">
        <input class="loginFormTextInput" type="text" placeholder="Username" name="username"/><br/>
        <input class="loginFormPasswordInput" type="password" placeholder="Password" name="password"/><br/>
        <input class="loginFormSubmitInput" type="submit" value="Login"/>
        <input class="loginFormButtonInput" type="button" value="Create Account"/><br/>
    </form>
</div>
